{
    "name": "nasty-codes-software/resonance",
    "description": "A private chat application with WebSocket support, built with PHP and ReactPHP.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "twig/twig": "^3.8",
        "cboden/ratchet": "^0.4",
        "react/socket": "^1.14",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "websocket": "php bin/websocket-server.php"
    }
}
